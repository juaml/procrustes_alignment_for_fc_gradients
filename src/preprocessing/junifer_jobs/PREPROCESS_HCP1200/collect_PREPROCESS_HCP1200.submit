
# The environment
universe = vanilla
getenv = True

# Resources
request_cpus = 1
request_memory = 20G
request_disk = 1G

# Executable
initial_dir = /data/project/identification_prediction/projects/gradients_identification_alignment/src/preprocessing/junifer_jobs/PREPROCESS_HCP1200
executable = $(initial_dir)/run_conda.sh
transfer_executable = False

arguments = gradient_identification junifer collect /data/project/identification_prediction/projects/gradients_identification_alignment/src/preprocessing/junifer_jobs/PREPROCESS_HCP1200/config.yaml --verbose 8 



# Logs
log = /data/project/identification_prediction/projects/gradients_identification_alignment/src/preprocessing/junifer_jobs/PREPROCESS_HCP1200/logs/junifer_collect.log
output = /data/project/identification_prediction/projects/gradients_identification_alignment/src/preprocessing/junifer_jobs/PREPROCESS_HCP1200/logs/junifer_collect.out
error = /data/project/identification_prediction/projects/gradients_identification_alignment/src/preprocessing/junifer_jobs/PREPROCESS_HCP1200/logs/junifer_collect.err
queue
